package baitap;
    import java.util.Objects;

    public class Vehicle {
        private int dungTich;
        private double triGia, thuePhaiNop;
        private String loaiXe,tenChuXe;

        public Vehicle(String tenChuXe, String loaiXe, int dungTich, double triGia, double thuePhaiNop) {
            this.tenChuXe = tenChuXe;
            this.dungTich = dungTich;
            this.triGia = triGia;
            this.loaiXe = loaiXe;
            this.thuePhaiNop = thuePhaiNop;
        }
        public Vehicle(){

        }

        public int getDungTich() {
            return dungTich;
        }

        public void setDungTich(int dungTich) {
            this.dungTich = dungTich;
        }

        public double getTriGia() {
            return triGia;
        }

        public void setTriGia(double triGia) {
            this.triGia = triGia;
        }

        public double getThuePhaiNop() {
            return thuePhaiNop;
        }

        public void setThuePhaiNop(double thuePhaiNop) {
            this.thuePhaiNop = thuePhaiNop;
        }

        public String getLoaiXe() {
            return loaiXe;
        }

        public void setLoaiXe(String loaiXe) {
            this.loaiXe = loaiXe;
        }

        public String getTenChuXe() {
            return tenChuXe;
        }

        public void setTenChuXe(String tenChuXe) {
            this.tenChuXe = tenChuXe;
        }


        public double tinhThue(){
            double thue;
            if(dungTich<100) thue=triGia*0.01;
            else if (dungTich >= 100 && dungTich<=200) thue = triGia * 0.03;
            else thue = triGia * 0.05;
            return thue;
        }

        @Override
        public String toString() {
            return "Vehicle{" + "dungTich=" + dungTich + ", triGia=" + triGia + ", thuePhaiNop=" + thuePhaiNop + ", loaiXe=" + loaiXe + ", tenChuXe=" + tenChuXe + '}';
        }



        void inThue(){
            System.out.printf("%5s %-20s %5d %10.2f %10.2f %8.5f \n ",tenChuXe,loaiXe,dungTich,triGia,thuePhaiNop,tinhThue());
        }
    }package baitap;
    import java.util.Objects;

    public class Vehicle {
        private int dungTich;
        private double triGia, thuePhaiNop;
        private String loaiXe,tenChuXe;

        public Vehicle(String tenChuXe, String loaiXe, int dungTich, double triGia, double thuePhaiNop) {
            this.tenChuXe = tenChuXe;
            this.dungTich = dungTich;
            this.triGia = triGia;
            this.loaiXe = loaiXe;
            this.thuePhaiNop = thuePhaiNop;
        }
        public Vehicle(){

        }

        public int getDungTich() {
            return dungTich;
        }

        public void setDungTich(int dungTich) {
            this.dungTich = dungTich;
        }

        public double getTriGia() {
            return triGia;
        }

        public void setTriGia(double triGia) {
            this.triGia = triGia;
        }

        public double getThuePhaiNop() {
            return thuePhaiNop;
        }

        public void setThuePhaiNop(double thuePhaiNop) {
            this.thuePhaiNop = thuePhaiNop;
        }

        public String getLoaiXe() {
            return loaiXe;
        }

        public void setLoaiXe(String loaiXe) {
            this.loaiXe = loaiXe;
        }

        public String getTenChuXe() {
            return tenChuXe;
        }

        public void setTenChuXe(String tenChuXe) {
            this.tenChuXe = tenChuXe;
        }


        public double tinhThue(){
            double thue;
            if(dungTich<100) thue=triGia*0.01;
            else if (dungTich >= 100 && dungTich<=200) thue = triGia * 0.03;
            else thue = triGia * 0.05;
            return thue;
        }

        @Override
        public String toString() {
            return "Vehicle{" + "dungTich=" + dungTich + ", triGia=" + triGia + ", thuePhaiNop=" + thuePhaiNop + ", loaiXe=" + loaiXe + ", tenChuXe=" + tenChuXe + '}';
        }



        void inThue(){
            System.out.printf("%5s %-20s %5d %10.2f %10.2f %8.5f \n ",tenChuXe,loaiXe,dungTich,triGia,thuePhaiNop,tinhThue());
        }
    }
